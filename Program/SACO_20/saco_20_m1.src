PUNTO[2]=XS2
PUNTO[3]=XS3
PUNTO[4]=XS4
PUNTO[5]=XS5
PUNTO[6]=XS6
PUNTO[7]=XS7
PUNTO[8]=XS8
PUNTO[9]=XS9
PUNTO[10]=XS10
PUNTO[11]=XS11
PUNTO[12]=XS12
;Endfold
