; === PROGRAMA PALETIZADO SACOS 20 KG MOSAICO 6-6 ==
;Fold VARIABLES
INC_Z1=400  ;Cota Inicial
INC_Z2=250  ;Cota Aproximacion 300
INC_Z3=0  ;Cota Inicial Dejada
PLANTA = 1
N_SACO=0
SACOS_A_PALETIZAR=SACOS_REALZIAR
n=1
