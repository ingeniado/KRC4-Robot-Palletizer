;Fold Compruebo Palet
WAIT FOR PALET_LIBRE OR SALIR_PROGRAMA_ACTUAL
IF SALIR_PROGRAMA_ACTUAL == TRUE THEN
   PULSE ( OK_LEIDO_SALIR_PROGRAMA, true, 0.5 )
   WAIT SEC 1
   GOTO SALIRPROG
ENDIF
;Endfold

BOMBA =TRUE
;=== BUCLE PRINCIPAL ==
FOR PLANTA=1 TO 5 ;Bucle de plantas
FOR n=1 TO 20 ;Bucle de cajas
CAJA:

;Fold Espero seÃ±al PLC:  REPITE_CAJA
IF (ERROR_SACO_CAIDO == TRUE ) THEN
   WAIT FOR REPITE_SACO OR SALIR_PROGRAMA_ACTUAL
   ERROR_SACO_CAIDO = FALSE
   
   IF SALIR_PROGRAMA_ACTUAL == TRUE THEN
   PULSE ( OK_LEIDO_SALIR_PROGRAMA, true, 0.5 )
   WAIT SEC 1
   GOTO SALIRPROG
   ENDIF

ENDIF
;Endfold
COGE_G () ;Llamo programa coger
;Fold CONDICION DE SALIDA DE PROGRAMA PLC
IF SALIR == TRUE THEN
   GOTO SALIRPROG
ENDIF
;Endfold

BAS(#BASE,2) ;Cambio de base
$APO.CPTP = 70

;Fold DEFINICION PUNTOS AUXILIARES
POS_0 =PUNTO[n]
POS_A =PUNTO[n]
POS_B =PUNTO[n]
POS_C =PUNTO[n]
POS_D =PUNTO[n]

POS_0.Y=POS_0.Y+80
POS_0.X=POS_0.X-80
POS_0.Z=POS_0.Z+INC_Z+700

POS_A.Y=POS_A.Y+80
POS_A.X=POS_A.X-80
POS_A.Z=POS_A.Z+INC_Z+300

POS_B.Z=POS_B.Z+INC_Z+60

POS_C.Z=POS_C.Z+INC_Z

POS_D.Z=POS_D.Z+INC_Z+300


;Endfold

;Fold Velocidad CAJA GRANDE
FOR ax_num=1 TO 6
$VEL_AXIS[ax_num]=vptpCAJAG
$ACC_AXIS[ax_num]=vaccCAJAG
ENDFOR
$VEL.CP=vlinCAJAG
;Endfold

PTP POS_0 C_PTP
PTP POS_A C_PTP

;Fold Velocidad Lenta
FOR ax_num=1 TO 6
$VEL_AXIS[ax_num]=vptpL
$ACC_AXIS[ax_num]=vaccL
ENDFOR
$VEL.CP=vlinL
;Endfold

LIN POS_B C_DIS
LIN POS_C

;Fold Compruebo caida de CAJA
IF (VACOESTATO == FALSE) THEN ;Condicion de saco perdido
   ERROR_SACO_CAIDO = TRUE
   FOR ax_num=1 TO 6
   $VEL_AXIS[ax_num]=vptpR
   $ACC_AXIS[ax_num]=vaccR
   ENDFOR
   $VEL.CP=vlinR
   LIN POS_A
   PTP POS_0 C_PTP
   ;FOLD PTP HOME  Vel= 100 % DEFAULT;%{PE}%MKUKATPBASIS,%CMOVE,%VPTP,%P 1:PTP, 2:HOME, 3:, 5:100, 7:DEFAULT
$BWDSTART = FALSE
PDAT_ACT=PDEFAULT
FDAT_ACT=FHOME
BAS (#PTP_PARAMS,100 )
$H_POS=XHOME
PTP  XHOME
;ENDFOLD
   GOTO CAJA
ENDIF
;Endfold
;Fold Abro vacio y compruebo
WAIT SEC 0.2
VACIO=FALSE  ;Abro vacio
WAIT FOR NOT VACOESTATO ;Espero Vacoestato
;Endfold
;Fold Velocidad Rapida
Continue
FOR ax_num=1 TO 6
$VEL_AXIS[ax_num]=vptpR
$ACC_AXIS[ax_num]=vaccR
ENDFOR
$VEL.CP=vlinR
;Endfold
PTP POS_D C_PTP
PTP POS_0 C_PTP

;Fold Incremento CAJA
N_SACO=N_SACO+1  
IF (N_SACO >= SACOS_A_PALETIZAR) THEN
   GOTO FINAL
ENDIF
;Endfold
ENDFOR
;Fold Incremento Planta
PLANTA = PLANTA + 1
INC_Z=INC_Z+500.0  ;Incremento cota superior
;Endfold
ENDFOR

GOTO FINAL
; Fold PUNTOS CAJAS
