;FOLD PTP C13 Vel=50 % PDAT13 Tool[1]:VENTOSA_SACOS Base[2]:PALETS;%{PE}%R 8.2.24,%MKUKATPBASIS,%CMOVE,%VPTP,%P 1:PTP, 2:C13, 3:, 5:50, 7:PDAT13
$BWDSTART=FALSE
PDAT_ACT=PPDAT13
FDAT_ACT=FC13
BAS(#PTP_PARAMS,50)
PTP XC13 
;ENDFOLD
